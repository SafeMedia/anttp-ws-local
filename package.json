{
  "name": "anttp-ws-local",
  "version": "1.0.0",
  "description": "Local WebSocket server for fetching files from autonomi network using anttp",
  "type": "module",
  "main": "dist/server.js",
"scripts": {
  "build": "tsc",
  "start:anttp": "node start-anttp.js",
  "start:server": "node dist/server.js",
  "start": "concurrently --kill-others \"npm run start:anttp\" \"npm run start:server\""
},
  "dependencies": {
    "node-fetch": "^3.3.2",
    "ts-node": "^10.0.0",
    "ws": "^8.0.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.5",
    "concurrently": "^9.1.2"
  }
}
